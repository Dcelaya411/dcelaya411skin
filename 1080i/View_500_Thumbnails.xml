<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<includes>

    <include name="View_500_Thumbnails">
        <control type="group">
            <include>Animation.Common</include>
            <visible>Control.IsVisible(500)</visible>
            <!--<include>JumpBarHorizontal</include>-->
            <include>ViewSlideForJumpBar</include>
            <control type="group">
                <control type="panel" id="500">
                    <oninfo condition="ListItem.IsCollection">ActivateWindow(1132)</oninfo>
                    <oninfo condition="Container.Content(actors)">RunScript(script.embuary.info,call=person,query='"$INFO[ListItem.Label]"')</oninfo>
                    <top>196</top>
                    <left>60</left>
                    <width>1800</width>
                    <height>680</height>
                    <onleft condition="!Skin.HasSetting(global.kioskmode)">9000</onleft>
                    <onright>1635</onright>
                    <onup>500</onup>
                    <ondown>500</ondown>
                    <preloaditems>0</preloaditems>
                    <pagecontrol>1635</pagecontrol>
                    <orientation>vertical</orientation>
                    <include content="forced_view" condition="Skin.HasSetting(enable.forcedviews)">
                        <param name="string" value="$LOCALIZE[21371]" />
                    </include>
                    <viewtype label="21371">list</viewtype>
                    <scrolltime tween="quadratic">400</scrolltime>
                    <itemlayout height="340" width="299">
                        <control type="image">
                            <left>10</left>
                            <top>11</top>
                            <width>93%</width>
                            <height>81%</height>
                            <texture border="10">common/$VAR[nofocus-shadow10]</texture>
                        </control>
                        <control type="label">
                            <left>20</left>
                            <right>20</right>
                            <bottom>10</bottom>
                            <height>50</height>
                            <align>center</align>
                            <font>Tiny</font>
                            <label>$INFO[ListItem.Label]</label>
                            <textcolor>Dark1</textcolor>
                        </control>
                        <control type="image">
                            <left>19</left>
                            <top>19</top>
                            <right>19</right>
                            <height>258</height>
                            <texture border="4" colordiffuse="Box2" background="true">common/box21.png</texture>
                            <visible>!Skin.HasSetting(thumbnails.white)</visible>
                        </control>
                        <control type="image">
                            <left>20</left>
                            <top>20</top>
                            <right>20</right>
                            <height>256</height>
                            <aspectratio scalediffuse="false">scale</aspectratio>
                            <texture diffuse="diffuse/wall.png" colordiffuse="ff181818">common/white.png</texture>
                        </control>
                        <control type="image">
                            <left>20</left>
                            <top>20</top>
                            <right>20</right>
                            <height>256</height>
                            <aspectratio scalediffuse="false" align="center" aligny="center">scale</aspectratio>
                            <texture diffuse="diffuse/wall.png" background="true">$VAR[ThumbImage]</texture>
                            <visible>!Container.Content(genres) + !Container.Content(studios)</visible>
                        </control>
                        <control type="image">
                            <left>20</left>
                            <top>20</top>
                            <right>20</right>
                            <height>256</height>
                            <aspectratio scalediffuse="false" align="center" aligny="center">keep</aspectratio>
                            <texture diffuse="diffuse/wall.png" fallback="DefaultFolder.png" background="true">$VAR[ThumbImageGenres]</texture>
                            <visible>Container.Content(genres)</visible>
                        </control>
                        <control type="image">
                            <left>20</left>
                            <top>20</top>
                            <right>20</right>
                            <height>256</height>
                            <aspectratio scalediffuse="false" align="center" aligny="center">keep</aspectratio>
                            <texture diffuse="diffuse/wall.png" fallback="DefaultFolder.png" background="true">$VAR[ThumbImageStudios]</texture>
                            <visible>Container.Content(studios)</visible>
                        </control>
                        <control type="label">
                            <align>center</align>
                            <aligny>center</aligny>
                            <width>50</width>
                            <right>13</right>
                            <top>12</top>
                            <height>50</height>
                            <textcolor>$VAR[WatchedLabelPosterColor]</textcolor>
                            <font>SymbolPoster</font>
                            <label>$VAR[WatchedLabelPoster]</label>
                            <visible>!String.IsEqual(ListItem.Label,..)</visible>
                        </control>
                    </itemlayout>
                    <focusedlayout height="340" width="299">
                        <control type="image">
                            <left>10</left>
                            <top>11</top>
                            <width>93%</width>
                            <height>81%</height>
                            <texture border="10">common/$VAR[nofocus-shadow10]</texture>
                        </control>
                        <control type="image">
                            <left>19</left>
                            <top>19</top>
                            <right>19</right>
                            <height>258</height>
                            <texture border="4" colordiffuse="Box2" background="true">common/box21.png</texture>
                            <visible>!Skin.HasSetting(thumbnails.white)</visible>
                        </control>
                        <control type="label">
                            <left>20</left>
                            <right>20</right>
                            <bottom>10</bottom>
                            <height>50</height>
                            <align>center</align>
                            <font>Tiny</font>
                            <label>$INFO[ListItem.Label]</label>
                            <textcolor>Dark1</textcolor>
                        </control>
                        <control type="image">
                            <left>20</left>
                            <top>20</top>
                            <right>20</right>
                            <height>256</height>
                            <aspectratio scalediffuse="false">scale</aspectratio>
                            <texture diffuse="diffuse/wall.png" colordiffuse="ff181818">common/white.png</texture>
                        </control>
                        <control type="image">
                            <left>20</left>
                            <top>20</top>
                            <right>20</right>
                            <height>256</height>
                            <aspectratio scalediffuse="false" align="center" aligny="center">scale</aspectratio>
                            <texture diffuse="diffuse/wall.png" background="true">$VAR[ThumbImage]</texture>
                            <visible>!Container.Content(genres) + !Container.Content(studios)</visible>
                        </control>
                        <control type="image">
                            <left>20</left>
                            <top>20</top>
                            <right>20</right>
                            <height>256</height>
                            <aspectratio scalediffuse="false" align="center" aligny="center">keep</aspectratio>
                            <texture diffuse="diffuse/wall.png" fallback="DefaultFolder.png" background="true">$VAR[ThumbImageGenres]</texture>
                            <visible>Container.Content(genres)</visible>
                        </control>
                        <control type="image">
                            <left>20</left>
                            <top>20</top>
                            <right>20</right>
                            <height>256</height>
                            <aspectratio scalediffuse="false" align="center" aligny="center">keep</aspectratio>
                            <texture diffuse="diffuse/wall.png" fallback="DefaultFolder.png" background="true">$VAR[ThumbImageStudios]</texture>
                            <visible>Container.Content(studios)</visible>
                        </control>
                        <control type="image">
                            <include>Animation.SelectBoxHome</include>
                            <left>13</left>
                            <top>13</top>
                            <right>13</right>
                            <bottom>56</bottom>
                            <texture border="20" colordiffuse="$VAR[ColorSquares]">common/selectbox.png</texture>
                        </control>
                        <control type="image">
                            <include>Animation.SelectBoxHome</include>
                            <left>13</left>
                            <top>13</top>
                            <right>13</right>
                            <bottom>56</bottom>
                            <texture border="20" colordiffuse="$VAR[ColorHighlightSelectbox2]">common/gradient-selectbox.png</texture>
                        </control>
                        <control type="label">
                            <align>center</align>
                            <aligny>center</aligny>
                            <width>50</width>
                            <right>13</right>
                            <top>12</top>
                            <height>50</height>
                            <textcolor>$VAR[WatchedLabelPosterColor]</textcolor>
                            <font>SymbolPoster</font>
                            <label>$VAR[WatchedLabelPoster]</label>
                            <visible>!String.IsEqual(ListItem.Label,..)</visible>
                        </control>
                    </focusedlayout>
                </control>
                <control type="group">
                    <left>SidePad</left>
                    <bottom>50</bottom>
                    <width>500</width>
                    <height>80</height>
                    <visible>String.IsEqual(ListItem.DBType,album) | String.IsEqual(ListItem.DBType,song)</visible>
                    <visible>false</visible>
                    <control type="image">
                        <posx>0</posx>
                        <width>30</width>
                        <height>30</height>
                        <texture colordiffuse="$VAR[ColorHighlight]">backg/$VAR[Flagstar331].png</texture>
                    </control>
                    <control type="image">
                        <posx>35</posx>
                        <width>30</width>
                        <height>30</height>
                        <texture colordiffuse="$VAR[ColorHighlight]">backg/$VAR[Flagstar332].png</texture>
                        <visible>!Skin.HasSetting(furniture.numericrating)</visible>
                    </control>
                    <control type="image">
                        <posx>70</posx>
                        <width>30</width>
                        <height>30</height>
                        <texture colordiffuse="$VAR[ColorHighlight]">backg/$VAR[Flagstar333].png</texture>
                        <visible>!Skin.HasSetting(furniture.numericrating)</visible>
                    </control>
                    <control type="image">
                        <posx>105</posx>
                        <width>30</width>
                        <height>30</height>
                        <texture colordiffuse="$VAR[ColorHighlight]">backg/$VAR[Flagstar334].png</texture>
                        <visible>!Skin.HasSetting(furniture.numericrating)</visible>
                    </control>
                    <control type="image">
                        <posx>140</posx>
                        <width>30</width>
                        <height>30</height>
                        <texture colordiffuse="$VAR[ColorHighlight]">backg/$VAR[Flagstar335].png</texture>
                        <visible>!Skin.HasSetting(furniture.numericrating)</visible>
                    </control>
                </control>
            </control>
            <control type="scrollbar" id="1635">
                <include content="scrollbar_vertical">
                    <param name="scrollid" value="1635"/>
                </include>
            </control>
        </control>
    </include>
</includes>